{
  "name": "mailcoach-server",
  "version": "1.0.0",
  "main": "./dist/src/server.js",
  "engines": {
    "node": ">=13.6.0"
  },
  "scripts": {
    "build": "tsc",
    "dev": "NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --inspect -- src/server.ts",
    "prestart": "node ./dist/src/bootstrap.js",
    "start": "node ./dist/src/server.js",
    "test": "NODE_ENV=test NODE_CONFIG_DIR=./config jest --watch --verbose .",
    "test:single": "NODE_ENV=test NODE_CONFIG_DIR=./config jest --watch --verbose",
    "test:ci": "npm run lint && NODE_ENV=test NODE_CONFIG_DIR=./config jest .",
    "lint": "eslint './**/*.{ts,js}'",
    "docs": "apidoc -i ./src -o ./_docs"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "config": "^3.3.6",
    "cookie-parser": "~1.4.3",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.4.0",
    "mkdirp": "^1.0.3",
    "moment": "^2.20.1",
    "node-cron": "^2.0.3",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.3.0",
    "rss-parser": "^3.12.0",
    "sequelize": "^5.22.4",
    "sequelize-migration-wrapper": "^0.1.1",
    "symlink-dir": "^4.1.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/config": "0.0.36",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.11",
    "@types/jsonwebtoken": "^8.5.1",
    "@types/mkdirp": "^1.0.1",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.14.37",
    "@types/node-cron": "^2.0.3",
    "@types/passport": "^1.0.6",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "apidoc": "^0.23.0",
    "eslint": "^7.23.0",
    "eslint-plugin-promise": "^4.3.1",
    "jest": "^26.1.0",
    "nock": "^13.0.11",
    "sequelize-fixtures": "^1.1.1",
    "sqlite3": "^5.0.2",
    "supertest": "^4.0.2",
    "ts-jest": "^26.5.4",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.1.6",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.2.3"
  },
  "apidoc": {
    "title": "MailCoach API Docs",
    "url": "http://localhost:3025"
  }
}
